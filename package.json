      "import-lazy": "^4.0.0",
      "mysql2": "^2.3.3",
      "nodemon": "^2.0.16",
      "react": "^18.1.0",
      "react-dom": "^18.1.0",
      "react-router-dom": "^6.3.0",
      "regenerator-runtime": "^0.13.9",
      "sequelize": "^6.21.0"
   },
   "devDependencies": {
      "@babel/core": "^7.17.10",
      "@babel/preset-env": "^7.17.10",
      "@babel/preset-react": "^7.16.7",
      "@testing-library/cypress": "^8.0.3",
      "@testing-library/react": "^13.2.0",
      "@testing-library/user-event": "^14.2.0",
